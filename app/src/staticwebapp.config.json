{ 
  "routes": [ 
    {
      "route": "/api/GetList",
      "rewrite": "/api/GetMessage"
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/facebook"
    },
    {
      "route": "/logout1",
      "rewrite": "/.auth/logout"
    },
    {
      "route": "/foo1",
      "redirect": "https://google.com/hi"
    },
    {
      "route": "/Foo3",
      "redirect": "https://google.com/foo"
    },
    {
      "route": "/bar",
      "statusCode": "301",
      "allowedRoles": ["admin"],
      "redirect": "/about"
    },
    {
      "route": "/Bar",
      "redirect": "/about"
    },
    {
      "route": "/about/*",
      "allowedRoles": ["authenticated", "admin"],
      "rewrite": "/about"
    },
    {
      "route": "/admin",
      "rewrite": "/admin/"
    },
    {
      "route": "/admin2",
      "rewrite": "/admin"
    },
    {
      "route": "/me",
      "redirect": "/.auth/me"
    },
    {
      "route": "/api/PostList",
      "methods": ["PUT", "POST"],
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "*.{png}" ]
  },
  "responseErrorOverrides": {
    "404": {
    },
    "401": {
      "redirect": "/.auth/login/facebook"
    }
  },
  "mimeTypes": { ".jpg": "application/json" },
  "globalHeaders": {
  "a": "b" },
   "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "enabled": true,
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/72f988bf-86f1-41af-91ab-2d7cd011db47",
          "clientId": "cf978515-3bd2-4632-9a88-84610d244ada",
          "clientSecretSettingName": "AAD_SECRET"
        }
      },
      "customOpenIdConnectProviders": {
        "azacdr": {
          "registration": {
            "clientId": "cf978515-3bd2-4632-9a88-84610d244ada",
            "clientCredential": {
              "clientSecretSettingName": "AAD_SECRET"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://login.microsoftonline.com/72f988bf-86f1-41af-91ab-2d7cd011db47/.well-known/openid-configuration"
            }
          },
          "login": {
              "scopes": [
              "openid profile email"
            ],
                "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name"
          }
        }
      }
    }
  }
} 
