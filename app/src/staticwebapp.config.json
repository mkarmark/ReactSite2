{ 
  "routes": [ 
    {
      "route": "/api/*",
      "methods": ["GET"],
      "rewrite": "/api/GetList"
    },
    {
      "route": "/api/*",
      "methods": ["POST"],
      "rewrite": "/api/GetList"
    },
    {
      "route": "/api/GetList",
      "rewrite": "/api/GetMessage"
    },
    {
      "route": "/*.jpg",
      "redirect": "https://www.google.com"
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/facebook"
    },
    {
      "route": "/logout1",
      "rewrite": "/.auth/logout"
    },
    {
      "route": "/foo1",
      "redirect": "https://google.com/hi"
    },
    {
      "route": "/Foo1",
      "redirect": "https://google.com/foo"
    },
    {
      "route": "/bar",
      "statusCode": "301",
      "allowedRoles": ["admin"],
      "redirect": "/about"
    },
    {
      "route": "/baz",
      "redirect": "/about"
    },
    {
      "route": "/about/*",
      "allowedRoles": ["authenticated", "admin"],
      "rewrite": "/about"
    },
    {
      "route": "/admin",
      "rewrite": "/admin"
    },
    {
      "route": "/me",
      "redirect": "/.auth/me"
    },
    {
      "route": "/api/PostList",
      "methods": ["PUT", "POST"],
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/api/GetList",
    "exclude": [ "*.{png}" ]
  },
  "responseErrorOverrides": {
    "404": {
    },
    "401": {
      "redirect": "/.auth/login/facebook"
    }
  },
  "mimeTypes": { ".jpg": "application/json" },
  "globalHeaders": {
  "a": "b" }
} 
